<form method="DELETE" action="{{ path('sondage_delete', {'id': sondage.id,'idEnqueteur': sondage.enqueteur.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ sondage.id) }}">
    <button class="btn">Delete</button>
</form>
